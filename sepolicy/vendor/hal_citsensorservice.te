type hal_citsensorservice_default, domain;

attribute hal_citsensorservice_server;
attribute hal_citsensorservice_client;
attribute hal_citsensorservice;

hal_server_domain(hal_citsensorservice_default, hal_citsensorservice)
binder_call(hal_citsensorservice_default, system_server);
type hal_citsensorservice_default_exec, exec_type, vendor_file_type, file_type;
init_daemon_domain(hal_citsensorservice_default)
hal_attribute_hwservice(hal_citsensorservice, hal_citsensorservice_hwservice)
get_prop(hal_citsensorservice_default, hwservicemanager_prop)
# Allow sensors to access backlight sysfs state
r_dir_file(hal_citsensorservice_default, vendor_sysfs_graphics)
allow hal_citsensorservice_default hal_citsensorservice_default_exec:file execute_no_trans;
allow hal_citsensorservice_default hidl_base_hwservice:hwservice_manager add;
allow hal_citsensorservice_default hwservicemanager:binder { call transfer };
allow hal_citsensorservice_default hal_citsensorservice_hwservice:hwservice_manager { add find };
